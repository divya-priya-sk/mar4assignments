#count of duplication of elements
l=[]
n=int(input())
for i in range(n):
    x=int(input())
    l.append(x)
print(l)
p=[]
count=0
for i in range(n):
    y=l[i]
    for j in range(i+1,n):
        z=l[j]
        if y==z:
            p.append(z)
res=[]
for i in p:
    if i not in res:
        res.append(i)
print("count of duplicate elements: ",len(res))

